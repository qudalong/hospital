
.img-list .item{
  position: relative;
 float: left;
 width: 233rpx;
 height: 233rpx;
 margin: 20rpx 0 0 16rpx;
 border-radius: 18rpx;
}
.img-list .item image{
 width:100%;
 height:100%;
}
.upload-btn{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 690rpx;
  margin: 30rpx;
  padding: 26rpx 0;
  box-sizing: border-box;
  color: #fff;
  text-align: center;
  background: #9999FF;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  font-size: 34rpx;
}
.delete-icon{
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  height: 60rpx;
  width: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 50%;
  color: #fff;
  background: #9999FF;
}